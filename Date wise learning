Date: 25th-oct-2023.
Topic: Hashing.
Q1. Union of two arrays.
Solution -> 
class Solution{
    public:
    //Function to return the count of number of elements in union of two arrays.
    int doUnion(int a[], int n, int b[], int m)  {
        unordered_set<int> s;
        for(int i=0;i<n;i++) {
            s.insert(a[i]);
        }
        for(int i=0;i<m;i++) {
            s.insert(b[i]);
        }
        return s.size();
    }
};
